        exampleZh: "效率是成功的关键。",
        audioUrl: "https://apifree.forvo.com/audio/3i232d3j1f2n381n1o2p34221h3c2k2d1h38213m1f273h1m3e3q3p3q3a1p3j1j273d2q232p2l3q273c21271m1p3m382m3535332i1i1l322n212g3d243a1p2h1h322j21323h2l321f1n393h3l2k1n3c3k2j1o3l2m23211t1t_1i252k361l243o1n222o2q2h3j351i3n3g263g2a3e371t1t",
        sourceArticle: "/posts/digest-2026-02-16-em-dash",
        sourceTitle: "技术日报：《Em dash》",
      }
    ],
  },
];
---

<Layout title={`Swedish Word Cards | ${SITE.title}`}>
  <Header />
  <main id="main-content" class="app-layout">
    <section class="prose prose-lg mx-auto max-w-4xl px-4 py-10">
      <h1 class="text-3xl font-bold mb-2">🇸🇪 Swedish Word Cards</h1>
      <p class="text-foreground/70 mb-8">
        Vocabulary organized by CEFR level — from beginner (A1) to upper-intermediate (B2).
      </p>

      {levels.map(({ level, color, bgColor, borderColor, words }) => (
        <div class="mb-10">
          <h2
            class="text-2xl font-bold mb-4 flex items-center gap-2"
            style={`color: ${color}`}
          >
            <span
              class="inline-block px-3 py-1 rounded-full text-sm font-bold text-white"
              style={`background-color: ${color}`}
            >
              {level}
            </span>
            Level {level}
          </h2>

          <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-4">
            {words.map(({ word, pronunciation, meaning, exampleSv, exampleZh, sourceArticle, sourceTitle, audioUrl, inflections, inflectionLabel, wordType, type }: any) => {
              // Resolve inflection display: prefer inflectionLabel string, fall back to inflections array
              const wtype = type || wordType || "";
              const typeIcon = wtype.startsWith("verb") ? "▸" : (wtype === "adjective" || wtype === "adj") ? "◈" : wtype ? "◆" : "";
              const typeShort = wtype.startsWith("noun_en") ? "en" : wtype.startsWith("noun_ett") ? "ett" : wtype.startsWith("verb") ? "verb" : wtype === "adjective" || wtype === "adj" ? "adj" : wtype.substring(0, 4) || "";
              // Build display label: use rich inflectionLabel if present, else join inflections array
              const displayInflections: string = inflectionLabel || (inflections && inflections.length > 0 ? [word, ...inflections].join(" · ") : "");
              return (
              <div
                id={word}
                class="rounded-xl p-5 flex flex-col gap-2 transition-transform hover:-translate-y-1"
                style={`background-color: ${bgColor}; border: 1px solid ${borderColor}`}
              >
                {/* Word + level badge + type pill */}
                <div class="flex items-start justify-between gap-2">
                  <div class="flex items-baseline gap-2 flex-wrap">
                    <span class="text-2xl font-bold text-foreground">{word}</span>
                    {typeShort && (
                      <span class="text-xs text-foreground/40 font-mono border border-foreground/20 px-1.5 py-0.5 rounded">
                        {typeShort}
                      </span>
                    )}
                  </div>
                  <span
                    class="text-xs font-bold px-2 py-0.5 rounded-full text-white shrink-0"
                    style={`background-color: ${color}`}
                  >
                    {level}
                  </span>
                </div>

                {/* Inflection label — full paradigm in one line */}
                {displayInflections && (
                  <p class="text-xs text-foreground/55 italic font-mono leading-snug">
                    {typeIcon && <span class="not-italic mr-1 text-foreground/30">{typeIcon}</span>}
                    {displayInflections}
                  </p>
                )}

                {/* Pronunciation + audio */}
                <div class="flex items-center gap-2 flex-wrap">
                  {pronunciation && pronunciation !== word && (
                    <p class="text-sm italic text-foreground/60">/{pronunciation}/</p>
                  )}
                  {audioUrl && (
                    <audio src={audioUrl} controls style="height:22px;vertical-align:middle"></audio>
                  )}
                </div>

                {/* Meaning */}
                <p class="text-xl font-medium text-foreground">{meaning}</p>

                {/* Example */}
                <div class="mt-2 pt-2 border-t border-foreground/20 text-sm text-foreground">
                  <p class="italic">{exampleSv}</p>
                  <p class="text-foreground/60">{exampleZh}</p>
                </div>

                {/* Source article link */}
                {sourceArticle && (
                  <a
                    href={sourceArticle}
                    class="mt-2 text-xs text-foreground/60 hover:text-foreground underline"
                  >
                    来源文章：{sourceTitle}
                  </a>
                )}
              </div>
            );
            })}
          </div>
        </div>
      ))}
    </section>
  </main>
  <Footer />
</Layout>
