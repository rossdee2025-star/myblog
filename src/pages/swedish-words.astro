                {displayInflections && (
                  <p class="text-xs text-foreground/55 italic font-mono leading-snug">
                    {typeIcon && <span class="not-italic mr-1 text-foreground/30">{typeIcon}</span>}
                    {displayInflections}
                  </p>
                )}

                {/* Pronunciation + audio */}
                <div class="flex items-center gap-2 flex-wrap">
                  {pronunciation && pronunciation !== word && (
                    <p class="text-sm italic text-foreground/60">/{pronunciation}/</p>
