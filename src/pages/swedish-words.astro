<Layout title={`Swedish Word Cards | ${SITE.title}`}>
  <Header />
  <main id="main-content" class="app-layout">
    <section class="prose prose-lg mx-auto max-w-4xl px-4 py-10">
      <h1 class="text-3xl font-bold mb-2">üá∏üá™ Swedish Word Cards</h1>
      <p class="text-foreground/70 mb-8">
        Vocabulary organized by CEFR level ‚Äî from beginner (A1) to upper-intermediate (B2).
      </p>

      {levels.map(({ level, color, bgColor, borderColor, words }) => (
        <div class="mb-10">
          <h2
            class="text-2xl font-bold mb-4 flex items-center gap-2"
            style={`color: ${color}`}
          >
            <span
              class="inline-block px-3 py-1 rounded-full text-sm font-bold text-white"
              style={`background-color: ${color}`}
            >
              {level}
            </span>
            Level {level}
          </h2>

          <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 gap-4">
            {words.map(({ word, pronunciation, meaning, exampleSv, exampleZh, sourceArticle, sourceTitle, audioUrl, inflections, wordType }: any) => (
              <div
                id={word}
                class="rounded-xl p-5 flex flex-col gap-2 transition-transform hover:-translate-y-1"
                style={`background-color: ${bgColor}; border: 1px solid ${borderColor}`}
              >
                {/* Word + level badge */}
                <div class="flex items-start justify-between">
                  <span class="text-2xl font-bold text-foreground">{word}</span>
                  <span
                    class="text-xs font-bold px-2 py-0.5 rounded-full text-white"
                    style={`background-color: ${color}`}
                  >
                    {level}
                  </span>
                </div>

                {/* Pronunciation + audio */}
                <div class="flex items-center gap-2 flex-wrap">
                  <p class="text-sm italic text-foreground/70">/{pronunciation}/</p>
                  {audioUrl && (
                    <audio src={audioUrl} controls style="height:22px;vertical-align:middle"></audio>
                  )}
                </div>

                {/* Inflections */}
                {inflections && inflections.length > 0 && (
                  <div class="text-xs text-foreground/50 font-mono flex flex-wrap gap-1">
                    <span class="text-foreground/30 mr-1">{wordType === "verb" ? "‚ñ∏" : wordType === "adj" ? "‚óà" : "‚óÜ"}</span>
                    {inflections.map((f: string) => (
                      <span class="bg-foreground/5 px-1.5 py-0.5 rounded">{f}</span>
                    ))}
                  </div>
                )}

                {/* Meaning */}
                <p class="text-xl font-medium text-foreground">{meaning}</p>

                {/* Example */}
                <div class="mt-2 pt-2 border-t border-foreground/20 text-sm text-foreground">
                  <p class="italic">{exampleSv}</p>
                  <p class="text-foreground/60">{exampleZh}</p>
                </div>

                {/* Source article link */}
                {sourceArticle && (
                  <a
                    href={sourceArticle}
                    class="mt-2 text-xs text-foreground/60 hover:text-foreground underline"
                  >
                    Êù•Ê∫êÊñáÁ´†Ôºö{sourceTitle}
                  </a>
                )}
              </div>
            ))}
          </div>
        </div>
      ))}
    </section>
  </main>
  <Footer />
</Layout>
