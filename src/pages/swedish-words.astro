<Layout title={`Swedish Word Cards | ${SITE.title}`}>
  <Header />
  <main id="main-content" class="app-layout">
    <section class="prose prose-lg mx-auto max-w-4xl px-4 py-10">
      <h1 class="text-3xl font-bold mb-2">ğŸ‡¸ğŸ‡ª Swedish Word Cards</h1>
      <p class="text-foreground/70 mb-8">
        Vocabulary organized by CEFR level â€” from beginner (A1) to upper-intermediate (B2).
      </p>

      {levels.map(({ level, color, bgColor, borderColor, words }) => (
        <div class="mb-10">
          <h2
            class="text-2xl font-bold mb-4 flex items-center gap-2"
            style={`color: ${color}`}
